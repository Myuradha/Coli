#include<stdio.h>
//#include<conio.h>
int main(){
	int nilai;
	printf("Masukkan nilai: ");
	scanf("%d", &nilai);
	if((nilai>=80) && (nilai<=100))
	{
		printf("grade A\n");
	} else if((nilai>=75) && (nilai<=79))
			{
				printf("grade B\n");
			} else if((nilai>=60) && (nilai<=74))
					{
						printf("grade C\n");
					} else if((nilai>=40) && (nilai<=59))
							{
								printf("grade D\n");
							} else if((nilai>=0) && (nilai<=39))
									{
										printf("grade E\n");
									} else 
									{
										printf("nilai salah\n");
									}
	return 0;
}

